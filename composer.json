{
  "name": "silktide/queueball-zmq",
  "description": "A ZeroMQ implementation for Silktide QueueBall, the message queue abstraction system",
  "license": "MIT",
  "homepage": "https://github.com/silktide/queueball-zmq",
  "require": {
    "php": ">=5.4.0",
    "ext-zmq": "*",
    "silktide/queueball": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "Silktide\\QueueBall\\ZeroMq\\": "src",
      "Silktide\\QueueBall\\ZeroMq\\Test\\": "test"
    }
  },
  "extra": {
    "downsider-puzzle-di": {
      "silktide/syringe": {
        "path": "config/syringe.yml"
      }
    }
  }
}