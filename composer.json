{
  "name": "lexide/queueball-zmq",
  "description": "A ZeroMQ implementation for Lexide QueueBall, the message queue abstraction system",
  "license": "MIT",
  "homepage": "https://github.com/lexide/queueball-zmq",
  "require": {
    "php": ">=5.4.0",
    "ext-zmq": "*",
    "lexide/queueball": "~3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*",
    "lexide/pharmacist": "~1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Lexide\\QueueBall\\ZeroMq\\": "src",
      "Lexide\\QueueBall\\ZeroMq\\Test\\": "test"
    }
  },
  "extra": {
    "lexide/puzzle-di": {
      "lexide/syringe": {
        "path": "config/syringe.yml"
      }
    }
  }
}
